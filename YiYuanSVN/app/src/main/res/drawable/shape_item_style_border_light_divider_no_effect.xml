<?xml version="1.0" encoding="utf-8"?>
<selector
    xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape>
            <solid android:color="@color/white"/>
            <stroke android:width="0.20000005dip" android:color="#60e6e6e6"/>
        </shape>
    </item>
    <item android:state_pressed="true">
        <shape>
            <solid android:color="@color/press_color_light"/>
            <stroke android:width="0.20000005dip" android:color="#60e6e6e6"/>
        </shape>
    </item>
</selector>